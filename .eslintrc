{
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true,
    "mocha": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "classes": true,
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "modules": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "import",
    "react",
    "react-hooks",
    "no-only-tests",
    "simple-import-sort",
    "@typescript-eslint/eslint-plugin"
  ],
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/extensions": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "import/parsers": {
      "@typescript-eslint/parser": [
        ".ts",
        ".tsx"
      ]
    }
  },
  "root": false,
  "rules": {
    "array-bracket-spacing": "off",
    "array-element-newline": [
      "warn",
      "consistent"
    ],
    "brace-style": "error",
    "comma-dangle": [
      "warn",
      "always-multiline"
    ],
    "comma-spacing": [
      "warn",
      {
        "after": true,
        "before": false
      }
    ],
    "complexity": [
      "warn",
      15
    ],
    "curly": "error",
    "import/named": "error",
    "import/no-unresolved": "off",
    "indent": "off",
    "@typescript-eslint/indent": [
      "warn",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "key-spacing": [
      "warn",
      {
        "afterColon": true,
        "align": "value",
        "beforeColon": false,
        "mode": "strict"
      }
    ],
    "keyword-spacing": [
      "warn",
      {
        "after": true,
        "before": true
      }
    ],
    "max-depth": [
      "error",
      2
    ],
    "max-len": [
      "warn",
      {
        "code": 100,
        "ignoreComments": true,
        "ignoreTemplateLiterals": true,
        "ignorePattern": "^import.+|^export.+",
        "ignoreTrailingComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "tabWidth": 2
      }
    ],
    "no-alert": "off",
    "no-console": [
      "error",
      {
        "allow": [
          "info",
          "warn",
          "error"
        ]
      }
    ],
    "no-multiple-empty-lines": [
      "warn",
      {
        "max": 2
      }
    ],
    "no-only-tests/no-only-tests": "error",
    "no-param-reassign": "error",
    "no-sparse-arrays": "error",
    "no-trailing-spaces": "warn",
    "no-unreachable": "error",
    "no-unused-vars": "off",
    "no-whitespace-before-property": "warn",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "ignoreRestSiblings": true
      }
    ],
    "object-curly-spacing": [
      "warn",
      "always"
    ],
    "padded-blocks": [
      "warn",
      {
        "classes": "always",
        "switches": "always"
      }
    ],
    "prefer-const": [
      "error",
      {
        "destructuring": "any"
      }
    ],
    "quote-props": [
      "warn",
      "as-needed"
    ],
    "quotes": [
      "warn",
      "single",
      "avoid-escape"
    ],
    "react/display-name": "off",
    "react/no-danger": "off",
    "semi": "off",
    "@typescript-eslint/semi": [
      "warn",
      "never"
    ],
    "semi-spacing": [
      "warn",
      {
        "after": true,
        "before": false
      }
    ],
    "sort-keys": [
      "off",
      "asc"
    ],
    "space-before-blocks": [
      "warn",
      "always"
    ],
    "space-before-function-paren": [
      "warn",
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "space-in-parens": [
      "warn",
      "never"
    ],
    "space-infix-ops": 1,
    "space-unary-ops": [
      "warn",
      {
        "nonwords": false,
        "words": true
      }
    ],
    "spaced-comment": [
      "warn",
      "always"
    ],
    "strict": "warn",
    "import/no-named-as-default": "off",
    "import/order": "off",
    "react-hooks/rules-of-hooks": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "react/prop-types": "warn",
    "react/no-unused-prop-types": "warn",
    "simple-import-sort/imports": "warn",
    "simple-import-sort/exports": "warn",
    "@typescript-eslint/no-use-before-define": "off",
    "@typescript-eslint/member-delimiter-style": [
      "warn",
      {
        "multiline": {
          "delimiter": "none"
        },
        "singleline": {
          "delimiter": "comma",
          "requireLast": false
        }
      }
    ],
    "@typescript-eslint/ban-ts-ignore": "off"
  },
  "overrides": [
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "rules": {
        "no-unused-vars": "warn",
        "@typescript-eslint/no-var-requires": "off",
        "@typescript-eslint/explicit-function-return-type": "off"
      }
    }
  ]
}
